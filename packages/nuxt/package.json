{
  "name": "@gqfn/nuxt",
  "type": "module",
  "version": "0.7.1",
  "description": "A Nuxt binding for @gqfn/core",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqfn.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "test": "vitest run",
    "test:full": "pnpm test:prepare && pnpm test:types && pnpm test",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "test:prepare": "pnpm dev:prepare"
  },
  "dependencies": {
    "@0no-co/graphql.web": "^1.1.2",
    "@gqfn/cli": "workspace:*",
    "@gqfn/core": "workspace:*",
    "@nuxt/kit": "^3.17.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "1.0.1",
    "@nuxt/schema": "^3.17.1",
    "@nuxt/test-utils": "^3.17.2",
    "@teages/nuxt-simple-graphql-client": "^0.0.3",
    "@types/node": "^22.15.3",
    "@types/ws": "^8.18.1",
    "consola": "^3.4.2",
    "eslint": "^9.25.1",
    "get-port-please": "^3.1.2",
    "nuxt": "^3.17.1",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10",
    "ws": "^8.18.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
