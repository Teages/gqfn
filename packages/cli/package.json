{
  "name": "@gqfn/cli",
  "type": "module",
  "version": "0.6.0",
  "description": "The CLI of GQFn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqfn.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "gqfn": "./dist/main.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "dev": "vitest dev",
    "build": "unbuild",
    "test": "vitest run --coverage",
    "test:full": "pnpm test:types && pnpm test",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:dev": "vitest run"
  },
  "peerDependencies": {
    "@gqfn/core": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@gqfn/core": {
      "optional": true
    }
  },
  "dependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "c12": "^3.0.3",
    "commander": "^13.1.0",
    "consola": "^3.4.2",
    "graphql": "^16.10.0",
    "importx": "^0.5.2",
    "magicast": "^0.3.5",
    "ofetch": "^1.4.1",
    "pathe": "^2.0.3",
    "tsx": "^4.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@pothos/core": "^4.5.1",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "graphql-scalars": "^1.24.2",
    "typescript": "^5.8.3",
    "unbuild": "3.5.0",
    "vitest": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
