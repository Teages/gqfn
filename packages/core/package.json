{
  "name": "@gqfn/core",
  "type": "module",
  "version": "0.6.0",
  "description": "Writing GraphQL queries with type safety and type hints, power by TypeScript.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqfn.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./schema": {
      "types": "./dist/schema.d.mts",
      "import": "./dist/schema.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "dev": "vitest dev",
    "build": "unbuild",
    "play": "cd playground && pnpm play",
    "play:gqfn": "cd playground && pnpm gqfn",
    "test": "vitest run --coverage",
    "test:full": "pnpm test:types && pnpm test",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:dev": "vitest run"
  },
  "peerDependencies": {
    "@gqfn/cli": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@gqfn/cli": {
      "optional": true
    }
  },
  "dependencies": {
    "graphql": "^16.10.0"
  },
  "devDependencies": {
    "@gqfn/cli": "workspace:*",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@teages/oh-my-graphql": "^0.1.0",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "unbuild": "3.5.0",
    "vitest": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
