{
  "name": "@gqfn/core",
  "type": "module",
  "version": "0.4.1",
  "description": "Writing GraphQL queries with type safety and type hints, power by TypeScript.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqfn.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "gqfn": "./dist/cli/main.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "dev": "vitest dev",
    "build": "unbuild",
    "play": "cd playground && pnpm play",
    "play:gqfn": "cd playground && pnpm gqfn",
    "test": "vitest run --coverage",
    "test:full": "pnpm test:types && pnpm test",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:dev": "vitest run"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "c12": "^2.0.4",
    "commander": "^13.1.0",
    "consola": "^3.4.0",
    "graphql": "^16.10.0",
    "importx": "^0.5.2",
    "ofetch": "^1.4.1",
    "ohash": "^1.1.4",
    "pathe": "^2.0.3",
    "tsx": "^4.19.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@pothos/core": "^4.4.0",
    "@shikijs/vitepress-twoslash": "^3.0.0",
    "@teages/oh-my-graphql": "0.0.2",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.6",
    "eslint": "^9.21.0",
    "graphql-request": "^7.1.2",
    "graphql-scalars": "^1.24.1",
    "typescript": "^5.7.3",
    "unbuild": "3.3.1",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
