{
  "name": "@gqfn/core",
  "type": "module",
  "version": "0.3.0",
  "description": "Writing GraphQL queries with type safety and type hints, power by TypeScript.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Teages/gqfn.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "import": "./dist/runtime/index.mjs",
      "require": "./dist/runtime/index.cjs"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.mjs",
      "require": "./dist/schema/index.cjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "gqfn": "./dist/cli/main.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "pnpm build",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "dev": "vitest dev",
    "build": "unbuild",
    "play": "cd playground && pnpm play",
    "play:gqfn": "cd playground && pnpm gqfn",
    "test": "vitest run --coverage",
    "test:full": "pnpm test:types && pnpm test",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:dev": "vitest run"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "c12": "^2.0.1",
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "graphql": "^16.9.0",
    "importx": "^0.5.0",
    "ofetch": "^1.3.4",
    "ohash": "^1.1.3",
    "pathe": "^1.1.2",
    "tsx": "^4.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@pothos/core": "^4.1.0",
    "@shikijs/vitepress-twoslash": "^1.13.0",
    "@teages/oh-my-graphql": "0.0.2",
    "@types/node": "^20",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^9.9.0",
    "graphql-request": "^7.1.0",
    "graphql-scalars": "^1.23.0",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7",
    "vitepress": "^1.3.2",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
